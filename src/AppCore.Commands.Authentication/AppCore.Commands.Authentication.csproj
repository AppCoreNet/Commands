<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net461</TargetFrameworks>
    <Description>Authentication support for AppCore .NET commands.</Description>
    <RootNamespace>AppCore.Commands</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AppCore.Diagnostics.Sources" Version="$(AppCore_Shared_Version)">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AppCore.Commands\AppCore.Commands.csproj" />
    <ProjectReference Include="..\AppCore.Commands.Authentication.Abstractions\AppCore.Commands.Authentication.Abstractions.csproj" />
  </ItemGroup>

</Project>